{
  "name": "web-widget-creator",
  "version": "1.0.0",
  "description": "IstSOS Web Widget Creator is a web application for generation of embeddable widgets, that display observation data from the IstSOS server.",
  "main": "dist/istsos-widget.min.js",
  "scripts": {
    "start": "webpack-dev-server --inline --hot --output-public-path=/dist/",
    "build": "webpack --config webpack.config.js && webpack --config webpack.widget.config.js",
    "build-widget": "webpack --config webpack.widget.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Luka-G/web-widget-creator.git"
  },
  "author": "Luka Glusica",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Luka-G/web-widget-creator/issues"
  },
  "homepage": "https://github.com/Luka-G/web-widget-creator#readme",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "istsos-javascript-core": "^1.0.3",
    "json-stringify-safe": "^5.0.1",
    "jsx-to-string": "^1.1.0",
    "moment": "^2.18.1",
    "openlayers": "^4.3.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "webpack": "^3.3.0",
    "webpack-closure-compiler": "^2.1.5",
    "webpack-dev-server": "^2.5.1"
  }
}
